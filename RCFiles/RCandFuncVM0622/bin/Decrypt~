#!/usr/bin/env python

import sys

#arguments = sys.argv[1:] # Gives list of command line arguments
encrypted = sys.argv[1]
#var = "hello"
counter, num = 0, 0
nums = []
passw = ""


for i in encrypted:
    if(counter < 5):
        num += ord(i)
    counter += 1
    if(counter == 5):
        nums.append(num / 5)
        passw += chr(num/5)
        num, counter = 0, 0

print passw
